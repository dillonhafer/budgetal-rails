$(".ajax-container.overview").html("<%= escape_javascript(render("overview")) %>");

<% if budget.amount_remaining < 0 %>
  $('#budget-remainder .progress').addClass('alert')
<% else %>
  $('#budget-remainder .progress').removeClass('alert')
<% end %>

var current_month = $('div.ajax-container.overview h4 .month-name').first().text().trim();
$('title').text(current_month+' - Budgetal.com');
